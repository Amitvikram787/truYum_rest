<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="menuItemList1" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="menuItem1" />
				<ref bean="menuItem2" />
				<ref bean="menuItem3" />
				<ref bean="menuItem4" />
				<ref bean="menuItem5" />
			</list>
        </constructor-arg>
	</bean>
	<bean id="menuItem1" class="com.cognizan.truyum.model.MenuItem">
		<property name="id" value="1" />
		<property name="name" value="SandWitch" />
		<property name="price" value="99" />
		<property name="active" value="true" />
		<property name="category" value="Main Course" />
		<property name="dateOfLaunch" value="2017/03/15" />
		<property name="freeDelivery" value="true" />
		<property name="imgSrc"
			value="https://images.unsplash.com/photo-1528735602780-2552fd46c7af?ixlib=rb-1.2.1&amp;ixid=eyJhcHBfaWQiOjEyMDd9&amp;auto=format&amp;fit=crop&amp;w=500&amp;q=60" />
	</bean>
	<bean id="menuItem2" class="com.cognizan.truyum.model.MenuItem">
		<property name="id" value="2" />
